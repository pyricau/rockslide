<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    >
    
    <ui:style>
     .big {
     font-size: 40px;
     }
    </ui:style>
    
    <g:HTMLPanel>
      <p class="{style.big}">Good old GWT <strong>DialogBox</strong></p>
      <g:Button addStyleNames="{style.big}" ui:field="closeButton">Close me!</g:Button>
    </g:HTMLPanel>
</ui:UiBinder>